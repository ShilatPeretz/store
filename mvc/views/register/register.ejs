<!DOCTYPE html>
<html lang="en" style="min-width: 400px;">
  <head>
    <link href="/CSS/login.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
  </head>
  <body>
    <!-- login & signup -->
    <div class="connect-div">
      <div class="links">
        <a href="#" id="login-form-link">Register</a>
      </div>

      <form class="login">
        <div class="inside-form">
        <div class="data-inputs">
          <input class="username" type="text" placeholder="Username" name="username" />
        </div>
        <div class="data-inputs">
            <input class="email" type="email" placeholder="Email" name="email"/>
          </div>
        <div class="data-inputs">
          <input class="password" type="password" placeholder="Password" name="password"/>
        </div>
        <div class="data-inputs">
          <button>Log in</button>
        </div>
      </div>
      <p class="errorMSG" style="color:red;text-align:center;"></p>
      </form>

    </div>

    <script src="https://code.jquery.com/jquery-3.6.1.min.js" integrity="sha256-o88AwQnZB+VDvE9tvIXrMQaPlFFSUTR+nldQm1LuPXQ=" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
    <script src="/JavaScript/register.js"></script>
</html>
